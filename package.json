{
  "name": "@hero-js/treegen",
  "version": "1.0.0",
  "description": "A tool for generating and visualizing directory tree structures.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "treegen": "dist/cli.js"
  },
  "scripts": {
    "test": "jest",
    "build": "rm -rf ./dist && tsc && cp -r ./dist/src/. ./dist && rm -rf ./dist/src",
    "win-build": "rmdir /s /q .\\dist && tsc && xcopy /s /e /Y .\\dist\\src dist && rmdir /s /q .\\dist\\src",
    "generate-doc": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hero-js/treegen.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "treegen",
    "hero",
    "herojs",
    "structure",
    "file",
    "file",
    "structure",
    "tree",
    "typescript"
  ],
  "author": "Sana Yasfp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hero-js/treegen/issues"
  },
  "homepage": "https://github.com/hero-js/treegen#readme",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "commander": "^11.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "directories": {
    "doc": "docs"
  }
}
